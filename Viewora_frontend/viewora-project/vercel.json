{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://viewora.duckdns.org/api/$1" },
    { "source": "/static/(.*)", "destination": "https://viewora.duckdns.org/static/$1" },
    { "source": "/media/(.*)", "destination": "https://viewora.duckdns.org/media/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
